    public void setData(ArrayList listdt) {      //tạo hàm đưa dữ liệu vào data
        DefaultTableModel tbdienthoai = new DefaultTableModel();
        tbdienthoai.addColumn("Ma Hang");
        tbdienthoai.addColumn("Ten");
        tbdienthoai.addColumn("Gia Ban");
        tbdienthoai.addColumn("Nha San Xuat");
        tbdienthoai.addColumn("Dung luong bo nho");
        tbdienthoai.addColumn("Mau sac");
     

        for (int i = 0; i < listdt.size(); i++) {
            DienThoai dt = (DienThoai) listdt.get(i);
   
            Object[] rowData = {dt.mahang, dt.ten, dt.giaban, dt.nhasanxuat, dt.dungluongbonho, dt.mausac};
            tbdienthoai.addRow(rowData);
        }

        tabledienthoai.setModel(tbdienthoai);
    }

